{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "template": "——",
          "type": "text"
        },
        {
          "foreground": "#ffffff",
          "background": "#0087af",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": true,
          "template": " {{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#005f87",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "properties": {
            "style": "full",
            "folder_separator_icon": " <b></b> "
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#95ffa4",
          "foreground": "#193549",
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "#db741a",
          "foreground": "#ffffff",
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "",
          "properties": {
            "threshold": 2000
          }
          
        },
        {
          "background": "#b72b27",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ .Code }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "—",
      "segments": [
        {
          "type": "text",
          "template": "—"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt"
    }
  ],
  "final_space": false,
  "version": 2
}
